<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="All Automation Practice Tests" parallel="tests" thread-count="2">

    <test name="Negative Login Tests">
        <parameter name="browser" value="FIREFOX"/>
        <classes>
            <class name="tests.FailedLoginWithoutRequiredDataTest"/>
        </classes>

    </test>

    <test name="Negative Login Tests v2">
        <parameter name="browser" value="CHROME"/>
        <classes>
            <class name="tests.FailedLoginWithoutTheRequiredNumberOfCharactersInThePasswordFieldTest"/>
        </classes>
    </test>

</suite>